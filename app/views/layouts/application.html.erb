<!DOCTYPE html>
<html>
  <head>
    <title>WebCrawlingApplication</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if current_user %>
    <%= link_to "Logout", destroy_user_session_path,class: "log_out btn btn-primary",method: :delete %>
    <% end %>
    <div class="container-fluid">
       <%= render 'layouts/messages' %>
      <%= yield %>
    </div>
    
  </body>
</html>
